; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "InputProxy",
"language": "GDScript",
"path": "res://script_gd/input.gd"
}, {
"base": "RigidBody",
"class": "Item",
"language": "GDScript",
"path": "res://script_gd/item.gd"
} ]
_global_script_class_icons={
"InputProxy": "",
"Item": ""
}

[application]

config/name="pygodotgame"
run/main_scene="res://scene/mainmenu.tscn"
boot_splash/image="res://icon.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

GDPORT="*res://script_gd/PORT.gd"
PYPORT="*res://script/PORT.py"

[display]

window/stretch/mode="2d"
window/stretch/aspect="expand"
window/stretch/shrink=1.5

[editor_plugins]

enabled=PoolStringArray(  )

[gdnative]

singletons=[ "res://pythonscript.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://resource/maintheme.tres"

[importer_defaults]

texture_3d={
"flags/filter": false
}
texture={
"flags/filter": false
}

[input]

repeat={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":82,"physical_scancode":0,"unicode":0,"echo":false,"script":null)
 ]
}

[network]

limits/debugger_stdout/max_chars_per_second=8192

[physics]

3d/smooth_trimesh_collision=true
common/physics_fps=180
3d/active_soft_world=false

[rendering]

environment/default_environment="res://default_env.tres"
